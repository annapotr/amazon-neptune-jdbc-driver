<?xml version='1.0' encoding='utf-8' ?>

<connector-plugin class='neptunejdbc' superclass='jdbc' plugin-version='1.13.0.0' name='SQL via Gremlin' version='18.1' min-version-tableau='2020.3'>
    <vendor-information>
        <company name="Amazon Neptune"/>
        <support-link url="https://docs.aws.amazon.com/neptune/latest/userguide/intro.html"/>
        <driver-download-link url="https://docs.aws.amazon.com/neptune/latest/userguide/intro.html"/>
    </vendor-information>
    <connection-customization class="neptunejdbc" enabled="true" version="10.0">
        <vendor name="Amazon Neptune"/>
        <driver name="SQL via Gremlin Connector"/>
        <customizations>
            <customization name="CAP_SUPPORTS_INITIAL_SQL" value="no" />
            <customization name='CAP_CREATE_TEMP_TABLES' value='no'/>
            <customization name='CAP_SUPPRESS_DISCOVERY_QUERIES' value='yes' />
            <customization name='CAP_SUPPRESS_DISPLAY_LIMITATIONS' value='yes'/>
            <customization name='CAP_QUERY_SUBQUERIES' value='yes'/>
            <customization name='CAP_QUERY_SUBQUERIES_WITH_TOP' value='no'/>
            <customization name='CAP_QUERY_TOP_N' value='no'/>
            <customization name='CAP_QUERY_TOPSTYLE_LIMIT' value='no'/>
            <customization name='CAP_QUERY_TOPSTYLE_TOP' value='no'/>
        </customizations>
    </connection-customization>
    <connection-fields file='connection-fields.xml'/>
    <connection-metadata file='connection-metadata.xml'/>
    <connection-resolver file="connectionResolver.tdr"/>
    <dialect file='dialect.tdd'/>
</connector-plugin>
